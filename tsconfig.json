{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "diagnostics": false,
        "declaration": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": false,
        "module": "CommonJS",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "ES5",
        "outDir": "./dist",
        "resolveJsonModule": true
    },
    "exclude": ["dist", "node_modules"],
    "include": ["./src/**/*.ts"],
    "files": ["src/build-react-routes.ts"]
}
